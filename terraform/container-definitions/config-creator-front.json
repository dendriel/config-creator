[
    {
      "name": "config-creator-front",
      "image": "registry.hub.docker.com/dendriel/config-creator-front:latest",
      "cpu": 10,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": { 
          "awslogs-group" : "/ecs/config-creator-front",
          "awslogs-region": "sa-east-1"
        }
      }
    }
]